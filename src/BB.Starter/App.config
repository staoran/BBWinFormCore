<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--软件名称-->
    <add key="ApplicationName" value="无马仓库管理系统" />
    <!--开发商名称-->
    <add key="Manufacturer" value="步兵软件科技发展有限公司" />
    <!--授权使用单位-->
    <add key="CertificatedCompany" value="步兵软件科技发展有限公司" />
    <!--软件介绍（关于对话框）-->
    <add key="Description" value="无马仓库管理系统，好用的备件仓库管理系统。具有备件信息管理、备件入库、备件出库、库存查询、库房管理、业务报表等功能，是一款优秀易用的仓库管理软件。" />
    <!--程序启动界面图片（不设置采用默认,建议图片宽高比2:1）-->
    <add key="SplashScreen" value="Images\startScreen.png" />
    <!--组件的数据库类型：access、sqlserver、sqlite、oracle等，默认为sqlserver可不写-->
    <add key="ComponentDbType" value="sqlserver" />
    <!--是否自动更新-->
    <add key="AutoUpdate" value="False" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <!-- 服务器升级路径 -->
    <!--<add key="ServiceLogPath" value="d:\MedicalCrmServiceLog"/>-->
    <!-- 定时执行任务，为false为不执行，为true执行 -->
    <add key="AutoRunTask" value="false" />
    <add key="debug" value="true" />
    <add key="db" value="Framework.DomainBase.DB, Framework.DomainBase" />
    <!-- 服务端地址 -->
    <add key="ip" value="127.0.0.1" />
    <!-- 服务连接端口 -->
    <add key="port" value="11260" />
    <!-- 以SOAP协议访问服务器 -->
    <!--<add key="channel" value="http"/>-->
    <add key="channel" value="tcp" />
    <!-- 客户端升级路径 -->
    <add key="ClientUpgradePath" value="d:\ClientUpgradePath" />
    <!-- 后端调用方式 soap,sql,wcf,api CallerFactory中使用-->
    <add key="CallerType" value="soap" />
    <!-- 指定代理类型 RPC中使用-->
    <add key="ProxyType" value="RemotingProxy" />
  </appSettings>
</configuration>